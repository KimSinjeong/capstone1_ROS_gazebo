
<launch>
  <arg name="x_pos" default="3.5"/>
  <arg name="y_pos" default="0.4"/>
  <arg name="z_pos" default="0.7"/>
  <arg name="roll" default="0"/>
  <arg name="pitch" default="0"/>
  <arg name="yaw" default="3.1415926535"/>
<arg name="model" default="waffle" doc="model type [burger, waffle, waffle_pi]"/>



  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find model16)/urdf/model16.urdf.xacro" />

  <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model model16 -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -R $(arg roll) -P $(arg pitch) -Y $(arg yaw) -param robot_description" />
</launch>

